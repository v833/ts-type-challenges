#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules:/mnt/c/Users/25073/Desktop/code/node_modules:/mnt/c/Users/25073/Desktop/node_modules:/mnt/c/Users/25073/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/typescript@4.5.4/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules/.pnpm/node_modules:/mnt/c/Users/25073/Desktop/code/ts-type-challenges/node_modules:/mnt/c/Users/25073/Desktop/code/node_modules:/mnt/c/Users/25073/Desktop/node_modules:/mnt/c/Users/25073/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
